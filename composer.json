{
  "name": "labrador-kennel/web-app",
  "description": "Skeleton for a typical Labrador framework application.",
  "license": ["MIT"],
  "minimum-stability": "beta",
  "require": {
    "php": "^8.1",
    "amphp/http-server-static-content": "^2.0@beta",
    "labrador-kennel/framework": "dev-main",
    "labrador-kennel/async-event-autowire": "dev-main",
    "league/plates": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "psalm/phar": "^5.11"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests"
    }
  }
}
