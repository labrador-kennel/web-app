<?xml version="1.0" encoding="UTF-8"?>
<annotatedContainer xmlns="https://annotated-container.cspray.io/schema/annotated-container.xsd">
  <scanDirectories>
    <source>
      <dir>src</dir>
      <dir>tests</dir>
    </source>
    <vendor>
      <package>
        <name>labrador-kennel/framework</name>
        <source>
          <dir>src</dir>
        </source>
      </package>
    </vendor>
  </scanDirectories>
  <definitionProviders>
    <definitionProvider>Labrador\AsyncEvent\Autowire\DefinitionProvider</definitionProvider>
    <definitionProvider>Labrador\Web\Autowire\DefinitionProvider</definitionProvider>
    <definitionProvider>App\Autowire\DefinitionProvider</definitionProvider>
  </definitionProviders>
  <observers>
    <observer>Labrador\AsyncEvent\Autowire\Observer</observer>
    <observer>Labrador\Web\Autowire\Observer</observer>
  </observers>
  <parameterStores>
    <parameterStore>config</parameterStore>
  </parameterStores>
  <!-- <cacheDir>.annotated-container-cache</cacheDir> -->
</annotatedContainer>
